{
    "name": "vagebond/envato-themecheck",
    "type": "library",
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "envato/envato-theme-check",
                "version":"master",
                "source": {
                    "url": "https://github.com/envato/envato-theme-check",
                    "type": "git",
                    "reference":"master"
                }
            }
        }
    ],
    "require": {
        "php": "^8.1",
        "symfony/console": "^6.2",
        "envato/envato-theme-check": "*@dev",
        "illuminate/support": "^10.9",
        "symfony/finder": "^6.2",
        "voku/simple_html_dom": "^4.8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Vagebnd\\EnvatoThemecheckCli\\": "src/"
        },
        "files": [
            "vendor/envato/envato-theme-check/checkbase.php"
        ],
        "classmap": [
            "vendor/envato/envato-theme-check/checks/"
        ]
    },
    "authors": [
        {
            "name": "Gianluca Riggio",
            "email": "luca@maximumawesome.nl"
        }
    ],
    "prefer-stable": true,
    "minimum-stability": "dev",
    "require-dev": {
        "laravel/pint": "dev-main",
        "symfony/var-dumper": "6.3.x-dev"
    },
    "bin": [
        "bin/check"
    ]
}
